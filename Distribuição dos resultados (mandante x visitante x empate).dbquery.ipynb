   "source": [
    "SELECT \n",
    "    CASE \n",
    "        WHEN mandante_placar > visitante_placar THEN 'Mandante'\n",
    "        WHEN mandante_placar < visitante_placar THEN 'Visitante'\n",
    "        ELSE 'Empate'\n",
    "    END AS resultado,\n",
    "\n",
    "    COUNT(*) AS qtd_jogos\n",
    "FROM fato_partida\n",
    "GROUP BY resultado\n",
    "ORDER BY qtd_jogos DESC;"
   ]
  }
